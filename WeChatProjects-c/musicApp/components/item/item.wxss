.songsheet_items_allbox{
  margin-top: 25rpx;
}
.songsheet_container_title{
  font-size: 17px;
  font-weight: bold;
  padding-left: 10rpx;
}
.songsheet_container_image{
  width: 95%;
  height: 220rpx;
  /* padding: 10rpx; */
  margin: 5rpx;
  border-radius: 8rpx;
}
.songsheet_container_text{
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 1;  
  -webkit-box-orient: vertical;
  font-size: 13px;
  margin-top: 10rpx;
  padding-left: 10rpx;
}
.songsheet_item_box{
  padding-top: 10rpx;
  width: 33%;
  display: inline-block;
  box-sizing: border-box;
}

.newsong_artists{
  font-size: 10px;
  padding-left: 10rpx;
  padding-top: -10rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 1;  
  -webkit-box-orient: vertical;

}
